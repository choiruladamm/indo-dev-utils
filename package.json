{
  "name": "@indodev/monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Monorepo for @indodev/toolkit library and documentation",
  "scripts": {
    "dev": "pnpm --filter @indodev/toolkit dev",
    "dev:docs": "pnpm --filter docs dev",
    "build": "pnpm --filter @indodev/toolkit build",
    "build:docs": "pnpm --filter docs build",
    "test": "pnpm --filter @indodev/toolkit test",
    "test:ui": "pnpm --filter @indodev/toolkit test:ui",
    "test:coverage": "pnpm --filter @indodev/toolkit test:coverage",
    "lint": "pnpm --filter @indodev/toolkit lint",
    "lint:fix": "pnpm --filter @indodev/toolkit lint:fix",
    "format": "pnpm --filter @indodev/toolkit format",
    "format:check": "pnpm --filter @indodev/toolkit format:check",
    "typecheck": "pnpm --filter @indodev/toolkit typecheck",
    "release": "pnpm --filter @indodev/toolkit release"
  },
  "keywords": [
    "monorepo",
    "indonesia",
    "utilities"
  ],
  "author": "Choirul Adam <choiruladamm@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/choiruladamm/indo-dev-utils.git"
  },
  "packageManager": "pnpm@9.0.0",
  "devDependencies": {
    "prettier": "^3.4.2"
  }
}